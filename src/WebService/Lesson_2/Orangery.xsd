<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <!-- Simple Types -->
    <xs:simpleType name="ColorType">
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SizeType">
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TemperatureType">
        <xs:restriction base="xs:string">
        </xs:restriction>
    </xs:simpleType>

    <!-- Complex Types -->
    <xs:complexType name="VisualParametersType">
        <xs:sequence>
            <xs:element name="StemColor" type="ColorType"/>
            <xs:element name="LeafColor" type="ColorType"/>
            <xs:element name="Size" type="SizeType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GrowingTipsType">
        <xs:sequence>
            <xs:element name="Temperature" type="TemperatureType"/>
            <xs:element name="Light" type="xs:string"/>
            <xs:element name="Watering" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PlantType">
        <xs:sequence>
            <xs:element name="Name" type="xs:string"/>
            <xs:element name="Soil" type="xs:string"/>
            <xs:element name="Origin" type="xs:string"/>
            <xs:element name="VisualParameters" type="VisualParametersType"/>
            <xs:element name="GrowingTips" type="GrowingTipsType"/>
            <xs:element name="Multiplying" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="OrangeryType">
        <xs:sequence>
            <xs:element type="PlantType" name="Plant" maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>
















